import java.util.Scanner;

class Main {
  public static void main (String [] args) {
    Scanner leitor = novo Scanner (System.in);
    System.out.println ("Informe o valor de x:");
    duplo x = leitor.nextDuplo ();
    System.out.println ("Informe o valor de y:");
    double y = leitor.nextDouble ();

    raiz dupla = Math.abs (y - (0,01 * Math.pow (x, 2)));
    duplo ValorRaiz = Math.sqrt (raiz);
    função dupla = 100 * ValorRaiz + 0,01 * (Math.abs (x + 10));

    System.out.println ("O cálculo da função obtida o valor:" função);
  }
}
